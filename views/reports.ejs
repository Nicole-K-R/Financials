<% active= "Reports" %>
<%- include templates/header.ejs %>
        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <!-- Percentage of Expenses (Month) -->
                    <div class="col-md-6">
                        <div class="card">
                            <div class="header">
                                <h4 class="title">Percentage of Expenses for <%= month %></h4>
                                <p class="category">Breakdown by expense type</p>
                            </div>
                            <div class="content">
                                <div id="monthExpenses" class="ct-chart ct-perfect-fourth"></div>

                                <div class="footer">
                                    <p style="font-size: 12px;">F = Food/Groceries, R =  Reports, Pe =  Personal, 
                                        E =  Entertainment, S =  School, M =  Miscellaneous, Pr =  Projects</p>
